{
    "manifest_version": 2,
    "name": "FullFocus",
    "description": "---",
    "version": "1.0",
    "content_scripts":[{
        "css": ["style.css"],
        "matches": [
            "https://www.youtube.com/*"
        ],
        "js": ["content.js"]
        }],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action":{
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs"
    ]

}